div.option-music#option-musicJS.hidden
  -for(i = 0; i < fileNames.length; i++)
    div.musicListBox
      input(type="radio", name="audio", id=fileNames[i]).musicRadioInputJS
      div.musicListBox_textBox
        div.musicListBox_name.musicNameJS #{fileNames[i]}
        div.musicListBox_time 00:00